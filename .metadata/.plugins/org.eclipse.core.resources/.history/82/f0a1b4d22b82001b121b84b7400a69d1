package DB;

public class STUTBL {
	
	private String name;
	private String gender;
	private int age;
	// 1 setter...
	// 2 생성자로...
	public STUTBL(String name,String gender,int age){
		this.name = name;
		this.gender = gender;
		this.age = age;
	}
	public void print(){
		System.out.println("이름은 "+name+"입니다");
		System.out.println("성별은 "+gender+"입니다");
		System.out.println("나이는 "+age+"입니다");
		System.out.println();
	}
}
